		this.catalog_filters = ko.observableArray($.map(catalog_filters, function(c, n) { return new Column(n, c.hasOwnProperty('index') ? c.index : 0, c.hasOwnProperty('name') ? c.name : "<unknown>", c.hasOwnProperty('qe_status') ? c.qe_status : 1, c.hasOwnProperty('display') ? c.display : 1, c.hasOwnProperty('editable') ? c.editable : 1); } ));
		this.catalog_attributes_status = ko.observable('{{ module_catalog_quick_edit_catalog_attributes_status }}');
		this.catalog_attributes_default_sort = ko.observable('{{ module_catalog_quick_edit_catalog_attributes_default_sort }}');
