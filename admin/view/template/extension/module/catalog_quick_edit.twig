		this.catalog_products_actions = ko.observableArray($.map(catalog_products_actions, function(c, n) { return new Column(n, c.hasOwnProperty('index') ? c.index : 0, c.hasOwnProperty('name') ? c.name : "<unknown>", c.hasOwnProperty('qe_status') ? c.qe_status : 1, c.hasOwnProperty('display') ? c.display : 1, c.hasOwnProperty('editable') ? c.editable : 1); } ));
		this.catalog_products_filter_sub_category = ko.observable('{{ module_catalog_quick_edit_catalog_products_filter_sub_category }}');
		this.catalog_filters_status = ko.observable('{{ module_catalog_quick_edit_catalog_filters_status }}');
		this.catalog_filters_default_sort = ko.observable('{{ module_catalog_quick_edit_catalog_filters_default_sort }}');
