		this.catalog_categories_default_order = ko.observable('{{ module_catalog_quick_edit_catalog_categories_default_order }}');
		this.catalog_categories = ko.observableArray($.map(catalog_categories, function(c, n) { return new Column(n, c.hasOwnProperty('index') ? c.index : 0, c.hasOwnProperty('name') ? c.name : "<unknown>", c.hasOwnProperty('qe_status') ? c.qe_status : 1, c.hasOwnProperty('display') ? c.display : 1, c.hasOwnProperty('editable') ? c.editable : 1); } ));
