		this.list_view_image_height = ko.observable('{{ module_catalog_quick_edit_list_view_image_height * 1 }}').extend({numeric: {precision: 0, context: self}, validate: {message: bull5i.texts.error_image_height, context: self, method: positiveInteger}});
