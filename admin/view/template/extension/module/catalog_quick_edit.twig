		error_image_width: '{{ error_image_width|addslashes() }}',
		error_image_height: '{{ error_image_height|addslashes() }}',
	});

	$(".sortable").sortable({
		containerSelector: 'table',
		itemPath: '> tbody',
		itemSelector: 'tr',
		placeholder: '<tr class="placeholder"/>',
		distance: 5,
		onDragStart: function ($item, container, _super, event) {
			$item.children().each(function() {
				$(this).width($(this).width());
			});
			$item.addClass("dragged");
			$("body").addClass("dragging");
		},
		onDrag: function ($item, position, _super, event) {
			position['left'] = 0;
			$item.css(position)
		},
		onDrop: function ($item, container, _super, event) {
			$item.children().each(function() {
				$(this).removeAttr("style");
			});
			$item.removeClass("dragged").removeAttr("style");
			$("body").removeClass("dragging");
			$("tbody tr", $(container.el[0])).each(function(i){
				var data = ko.dataFor(this);
				data.index(i);
			})
		},
	});

	bull5i.load_service_list = function(force) {
		var force = force !== undefined ? force * 1 : 0,
			dfd = $.Deferred();
		if ((!ExtVM.service_list_loaded() || force) && !ExtVM.service_list_loading()) {
			ExtVM.service_list_loading(true);
			$.when($.ajax({
				url: '{{ services }}',
				data: {force: force},
				dataType: 'json'
			})).then(function(data) {
				ExtVM.service_list_loaded(true);
				ExtVM.service_list_loading(false);
				ExtVM.clearServices();
				if (data.services) {
					$.each(data.services, function(i, service) {
						var code = service.code,
							name = service.name,
							description = service.description || "",
							currency = service.currency,
							price = service.price,
							turnaround = service.turnaround;
						ExtVM.addService(code, name, description, currency, price, turnaround);
					})
				}
				if (data.rate) {
					$("#hourly_rate").html(data.rate);
				}
				dfd.resolve();
			}, function (jqXHR, textStatus, errorThrown) {
				ExtVM.service_list_loaded(true);
				ExtVM.service_list_loading(false);
				dfd.reject();
				if (window.console && window.console.log) {
					window.console.log("Failed to load services list: " + errorThrown)
				}
			});
		} else {
			dfd.reject();
		}
		return dfd.promise();
	}

	var positiveInteger = function(newValue) {
		if (isNaN(parseInt(newValue)) || parseInt(newValue) < 0) {
			this.target.hasError(true);
			this.target.errorMsg(this.message);
		} else {
			this.target.hasError(false);
			this.target.errorMsg("");
		}
	}

	var Service = function(code, name, description, currency, price, turnaround) {
		this.code = code;
		this.name = name;
		this.description = description;
		this.currency = currency;
		this.price = price;
		this.turnaround = turnaround;
	}

	var Column = function(column, index, name, editable, visible, quick_editable) {
		this.column = column;
		this.index = ko.observable(index);
		this.name = name;
		this.editable = ko.observable(editable);
		this.visible = ko.observable(visible);
		this.quick_editable = quick_editable;
	}

	var ModuleViewModel = function() {
